conc:SecurityRoleTypes a skos:ConceptScheme ;
	skos:prefLabel "Security role types"@en .

conc:SecurityRoleTypes-Read a skos:Concept ;
 	skos:inScheme conc:SecurityRoleTypes ;
 	skos:prefLabel "Read role"@en ;
 	skos:altLabel "Read" ;
 	skos:exactMatch conc:SepBaseRoles-Consumer ;
 	skos:definition "Security role that allows read rights to the assigned users on the given Entity"@en .
 	
conc:SecurityRoleTypes-Read-Write a skos:Concept ;
	skos:inScheme conc:SecurityRoleTypes ;
	skos:prefLabel "Write role"@en ;
	skos:altLabel "Read-Write" ;
	skos:exactMatch conc:SepBaseRoles-Collaborator, conc:SepBaseRoles-Contributor, conc:SepBaseRoles-Creator ;
	skos:definition "Security role that allows read and write rights to the assigned users on the given Entity"@en .
	
conc:SecurityRoleTypes-Manager a skos:Concept ;
	skos:inScheme conc:SecurityRoleTypes ;
	skos:prefLabel "Manager role"@en ;
	skos:altLabel "Manager" ;
	skos:exactMatch conc:SepBaseRoles-Manager ;
 	skos:definition "Security role that gives manager permissions to the assigned users on the given Entity"@en .
 	
conc:SecurityRoleTypes-Forbidden a skos:Concept ;
	skos:inScheme conc:SecurityRoleTypes ;
	skos:prefLabel "Forbidden role"@en ;
	skos:altLabel "Forbidden" ;
	skos:exactMatch conc:SepBaseRoles-NoPermission ;
 	skos:definition "Security role that forbids access to the assigned users on the given Entity"@en .
 	
conc:SepBaseRoles a skos:ConceptScheme;
	skos:prefLabel "Security role types in SEP"@en .
	
conc:SepBaseRoles-Administrator a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "ADMINISTRATOR"@en ;
 	skos:altLabel "100"^^xsd:integer ;
 	skos:definition "The administrator. Full permission for anything."@en .

conc:SepBaseRoles-Manager a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "MANAGER"@en ;
 	skos:altLabel "99"^^xsd:integer ;
 	skos:definition "The manager. Almost full permissions, but could be less than administrator"@en .
 	
conc:SepBaseRoles-Collaborator a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "COLLABORATOR"@en ;
 	skos:altLabel "15"^^xsd:integer ;
 	skos:definition "The collaborator. Has edit permissions on the content created by him and on the content created by other users. Has delete permissions only on the content created by him."@en .
 	
conc:SepBaseRoles-Contributor a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "CONTRIBUTOR"@en ;
 	skos:altLabel "9"^^xsd:integer ;
 	skos:definition "The contributor. Has edit/ delete permissions on the content he created."@en .
 	
conc:SepBaseRoles-Creator a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "CREATOR"@en ;
 	skos:altLabel "10"^^xsd:integer ;
 	skos:definition "The creator. This role is relevant only for object that are not part of permission model - tasks, workflows, comments, topics, relations, etc. This is the user who created an object. For example the creator of a task could create sub-task of this task"@en .
 	
conc:SepBaseRoles-Consumer a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "CONSUMER"@en ;
 	skos:altLabel "5"^^xsd:integer ;
 	skos:definition "The consumer. Has view only permissions on the content of a instances. Cannot create new content (upload documents, create cases, assign tasks, etc.)"@en .
 	
conc:SepBaseRoles-Viewer a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "VIEWER"@en ;
 	skos:altLabel "0"^^xsd:integer ;
 	skos:definition "The viewer. Knows only content exists, but has no permission to see it"@en .
 	
conc:SepBaseRoles-NoPermission a skos:Concept ;
 	skos:inScheme conc:SepBaseRoles ;
 	skos:prefLabel "NO_PERMISSION"@en ;
 	skos:altLabel "-1"^^xsd:integer ;
 	skos:definition "The user is forbidden to access the Entity and all of its children, if they inherit parent's permission"@en .
 	
