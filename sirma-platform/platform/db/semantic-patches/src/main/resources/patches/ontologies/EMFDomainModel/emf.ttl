###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#blocks

emf:blocks rdf:type owl:ObjectProperty , emf:DefinitionObjectProperty , emf:DefinitionProperty ;
           
           rdfs:label "Blocks"@en ,
                      "Блокира"@bg ;
           
           dc:creator "Kiril Penev"@en ;
           
           skos:definition "Specifies which Activity is blocked by the current Activity"@en ;
           
           rdfs:domain emf:Activity ;
           
           rdfs:range emf:Activity ;
           
           owl:inverseOf emf:isBlockedBy .
           

           
           
###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isBlockedBy

emf:isBlockedBy rdf:type owl:ObjectProperty , emf:DefinitionObjectProperty , emf:DefinitionProperty ;
              
           rdfs:label "Is Blocked By"@en ,
                      "Е блокиран от"@bg ;
              
           skos:definition "Denotes that an activity is blocked by another activity"@en ;
              
           dc:creator "Valeri Tishev"@en ;
           
           rdfs:domain emf:Activity ;
              
           rdfs:range emf:Activity ;
              
           owl:inverseOf emf:blocks .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#commentsOn

emf:commentsOn rdf:type owl:ObjectProperty , emf:DefinitionObjectProperty , emf:DefinitionProperty ;
               
               rdfs:label "Comments on"@en ;
               
               dc:creator "Kiril Penev"@en ;
               
               emf:isSystemProperty "true"^^xsd:boolean ;
               
               skos:definition "Specifies which object is commented or discussed by a Comment"@en ;
               
               rdfs:domain emf:Discussion ;
               
               rdfs:range ptop:Entity .


###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#completedBy

emf:completedBy rdf:type owl:ObjectProperty , emf:DefinitionObjectProperty , emf:DefinitionProperty ;
                
                rdfs:label "Completed by"@en ,
                           "Завършен от"@bg ;
                
                emf:isSystemProperty "true"^^xsd:boolean ;
                
                dc:creator "Kiril Penev"@en ;
                
                skos:definition "User that completed the activity"@en ;
                
                rdfs:domain emf:Activity ;
                
                rdfs:range ptop:Agent .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#contactPerson

emf:contactPerson rdf:type owl:ObjectProperty , emf:DefinitionObjectProperty , emf:DefinitionProperty ;
                  
                  rdfs:label "Contact Person"@en ,
                             "Лице за контакт"@bg ;
                  
                  skos:definition "A person who provides a link for information or representation between two parties."@en ;
                  
                  dc:creator "Kiril Penev"@en ;
                  
