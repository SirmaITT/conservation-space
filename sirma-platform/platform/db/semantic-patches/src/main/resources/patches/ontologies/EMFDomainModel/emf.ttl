#################################################################
#
#    Annotation properties
#
#################################################################



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#auditEvent

emf:auditEvent rdf:type owl:AnnotationProperty ;

                 rdfs:label "Audit event"@en ;
                 
                 skos:definition "The audit event identifier that corresponds to given object or data property"@en ;
                  
                 dc:creator "Borislav Bonev"@en ;
                 
                 rdfs:domain emf:DefinitionProperty ;
                
                 rdfs:range xsd:string .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#definitionId

emf:definitionId rdf:type owl:AnnotationProperty ;
                 
                 rdfs:label "Definition id"@en ,
                            "definition id"@en ;
                 
                 skos:definition "Id of the definition of the object"@en ;
                 
                 dc:creator "Kiril Penev"@en .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isPartOfObjectLibrary

emf:isPartOfObjectLibrary rdf:type owl:AnnotationProperty ;
                          
                          rdfs:label "Is part of object library"@en ;
                          
                          skos:definition "Flag that shows if the instance type is part of the object library"@en ;
                          
                          dc:creator "Kiril Penev"@en .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isSearchable

emf:isSearchable rdf:type owl:AnnotationProperty ;
                 
                 rdfs:label "Is searchable"@en ;
                 
                 skos:definition "Can be used as an object type when executing a search "@en ;
                 
                 dc:creator "Kiril Penev"@en ;
                 
                 rdfs:range xsd:boolean .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#acceptDataTypePattern

emf:acceptDataTypePattern rdf:type owl:AnnotationProperty ;
                 
                 rdfs:label "Datatype pattern"@en ;
                 
                 skos:definition "Regex pattern that can be used to describe class that is more applicable to files of given mimetype"@en ;
                 
                 dc:creator "Borislav Bonev"@en ;
                 
                 rdfs:range xsd:string .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isCreateable

emf:isCreateable rdf:type owl:AnnotationProperty ;
                 
                 rdfs:label "Is createable"@en ;
                 
                 skos:definition "A mark for a class that can be used to create new instances from the system"@en ;
                 
                 dc:creator "Borislav Bonev"@en ;
                 
                 rdfs:range xsd:boolean .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isUploadable

emf:isUploadable rdf:type owl:AnnotationProperty ;
                 
                 rdfs:label "Is uploadable"@en ;
                 
                 skos:definition "Can be used as an object type to upload an instance of it"@en ;
                 
                 dc:creator "Borislav Bonev"@en ;
                 
                 rdfs:range xsd:boolean .



###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isSystemProperty

emf:isSystemProperty rdf:type owl:AnnotationProperty ;
                     
                     rdfs:label "Is system property"@en ;
                     
                     skos:definition "Indicates whether a property is applicable for end-user interaction."@en ;
                     
                     rdfs:comment "System properties are used to encode system specific information. They, as well as their instances and related information, should usually not be presented to the end-user. In practice, user-interface and visualization modules, can filter such entities."@en ;
                     
                     dc:creator "Valeri Tishev"@en ;
                     
                     rdfs:range xsd:boolean .


###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isVersionable

emf:isVersionable rdf:type owl:AnnotationProperty ;
                     
                     rdfs:label "Is versionable"@en ;
                     
                     skos:definition "Used as marker to show if instance is versionable or not. Primary used in content versioning."@en ;
                     
                     dc:creator "Tony Kunchev"@en ;
                     
                     rdfs:range xsd:boolean .


###  http://purl.org/dc/elements/1.1/creator

dc:creator rdf:type owl:AnnotationProperty .





