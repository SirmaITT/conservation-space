								<ui-param id="itemType" name="ITEM_TYPE">all</ui-param>
								<ui-param id="pklMode" name="FUNCTIONAL_MODE">multy</ui-param>
								<ui-param id="okBtnTitle" name="OK_BUTTON_TITLE">assignee.okBtn.title</ui-param>
