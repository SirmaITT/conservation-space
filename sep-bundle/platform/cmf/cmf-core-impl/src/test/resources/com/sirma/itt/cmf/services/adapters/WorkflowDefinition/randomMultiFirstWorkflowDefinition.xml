						<ui-param id="cancelBtnTitle" name="CANCEL_BUTTON_TITLE">assignee.cancelBtn.title</ui-param>
						<ui-param id="picklistHeaderTitle" name="HEADER_TITLE">multiAssignees.picklistHeader.title</ui-param>
					</control>
				</field>
			</fields>
