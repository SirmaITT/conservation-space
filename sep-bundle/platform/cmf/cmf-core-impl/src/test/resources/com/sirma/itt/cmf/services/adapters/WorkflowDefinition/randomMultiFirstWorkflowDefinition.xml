		<label id="multiAssignees.triggerBtn.title">
			<value lang="bg">Изберете потребители</value>
			<value lang="en">Select users</value>
		</label>
		<label id="multiAssignees.picklistHeader.title">
			<value lang="bg">Списък потребители</value>
			<value lang="en">Select users</value>
		</label>
	</labels>
</workflowDefinition>