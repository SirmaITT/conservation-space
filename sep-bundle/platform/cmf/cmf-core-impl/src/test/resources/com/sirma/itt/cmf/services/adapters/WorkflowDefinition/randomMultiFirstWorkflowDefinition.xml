				<region id="taskActions" order="5" label="tasks.taskActions">
					<fields>
						<field name="assignee" type="an..50" mandatory="false" displayType="system" dmsType="bpm:assignee" />
					</fields>
				</region>
			</regions>
			<transitions>
				<transition id="RT0095" label="startEndTask.new_task.btn.title" eventId="RANDOM_NEW_TASK">
					<fields>
						<field name="parallelAssignees" order="21" type="an..1024" label="WFTYPE9M1.parallelAssignees" mandatory="false" displayType="editable"
							previewEmpty="false" dmsType="bpm:assignees" multiValued="true">
							<control id="PICKLIST">
								<control-param id="assigneeListItemsFilter" name="FILTER_NAME">groupAssigneeListItemsFilter</control-param>
