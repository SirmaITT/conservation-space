            <value lang="en"><![CDATA[${eval(<span class="truncate-element"><a class="${get([status])} task-state-${get([status])} has-tooltip" href="${link(currentInstance)}" uid="${id.uid}">${id.uid} ${CL([type])} (${CL([status])})</a></span>)}]]></value>
		</label>
        <label id="TASK_BREADCRUMB_HEADER">
			<value lang="bg"><![CDATA[${eval(<a class="${get([status])} task-state-${get([status])} has-tooltip" href="${link(currentInstance)}" uid="${id.uid}">${CL([type])}</a>)}]]></value>
            <value lang="en"><![CDATA[${eval(<a class="${get([status])} task-state-${get([status])} has-tooltip" href="${link(currentInstance)}" uid="${id.uid}">${CL([type])}</a>)}]]></value>
		</label>
