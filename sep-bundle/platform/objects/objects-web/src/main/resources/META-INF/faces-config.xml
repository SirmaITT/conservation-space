<?xml version="1.0"?>
<!-- Marker file indicating JSF 2.0 should be enabled in the application -->
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="       http://java.sun.com/xml/ns/javaee       http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<!-- object landing page -->
		<navigation-case>
			<from-outcome>objectinstance</from-outcome>
			<to-view-id>/object/object.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>object</from-outcome>
			<to-view-id>/object/object.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- case objects tab -->
		<navigation-case>
			<from-outcome>case-objects</from-outcome>
			<to-view-id>/case/case-objects.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!--  -->
		<navigation-case>
			<from-outcome>clone-object</from-outcome>
			<to-view-id>/object/clone-object.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
</faces-config>
