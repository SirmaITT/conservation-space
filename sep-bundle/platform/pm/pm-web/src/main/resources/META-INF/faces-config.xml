<?xml version="1.0"?>
<!-- Marker file indicating JSF 2.0 should be enabled in the application -->
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xi="http://www.w3.org/2001/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="       http://java.sun.com/xml/ns/javaee       http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
	<navigation-rule>
		<from-view-id>*</from-view-id>

		<!-- project landing page navigation -->
		<navigation-case>
			<from-outcome>projectinstance</from-outcome>
			<to-view-id>/project/project.xhtml</to-view-id>
			<redirect />
		</navigation-case>
		
		<!-- project result list navigation -->
		<navigation-case>
			<from-outcome>project-list</from-outcome>
			<to-view-id>/project/project-list.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- project dashboard -->
		<navigation-case>
			<from-outcome>project-dashboard</from-outcome>
			<to-view-id>/project/project-dashboard.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- manage project resources navigation -->
		<navigation-case>
			<from-outcome>manage-resources</from-outcome>
			<to-view-id>/project/manage-resources.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- project relations navigation -->
		<navigation-case>
			<from-outcome>project-relations</from-outcome>
			<to-view-id>/project/project-relations.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- project schedule navigation -->
		<navigation-case>
			<from-outcome>project-schedule</from-outcome>
			<to-view-id>/project/project-schedule.xhtml</to-view-id>
			<redirect />
		</navigation-case>

		<!-- project resource allocation navigation -->
		<navigation-case>
			<from-outcome>project-resource-allocation</from-outcome>
			<to-view-id>/project/project-resource-allocation.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>


</faces-config>
