###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#definitionId

emf:definitionId rdf:type owl:AnnotationProperty ;
                 
                 rdfs:label "Definition id"@en ,
                            "definition id"@en ;
                 
                 skos:definition "Id of the definition of the object"@en ;
                 
