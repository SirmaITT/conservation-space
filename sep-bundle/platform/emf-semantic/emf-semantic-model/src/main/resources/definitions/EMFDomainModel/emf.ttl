###  http://ittruse.ittbg.com/ontology/enterpriseManagementFramework#isPartOfObjectLibrary

emf:isPartOfObjectLibrary rdf:type owl:AnnotationProperty ;
                          
                          rdfs:label "Is part of object library"@en ;
                          
                          skos:definition "Flag that shows if the instance type is part of the object library"@en ;
                          
