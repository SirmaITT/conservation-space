<web-fragment xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-fragment_3_0.xsd"
	version="3.0" id="emf-core-web">
	
	<name>emf-core-web</name>

	<!-- Context parameters -->
	<context-param>
		<param-name>javax.faces.FACELETS_LIBRARIES</param-name>
		<param-value>/WEB-INF/emf.taglib.xml</param-value>
	</context-param>

	<context-param>
		<description>Excluded paths from security check</description>
		<param-name>com.sirma.itt.cmf.security.sso.excludePaths</param-name>
		<param-value>/javax.faces.resource,/rfRes</param-value>
	</context-param>

</web-fragment>