###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/NGA#associatedWith

nga:associatedWith rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ;
                   
                   rdfs:label "is associated with"@en ;
                   
                   emf:isSearchable "true"^^xsd:boolean ;
                   
                   dc:creator "Lidiya Dimitrova" ;
                   
                   emf:auditEvent "+addAssociatedWith|-removeAssociatedWith|changeAssociatedWith" ;
                   
                   skos:definition "Indicates the cultural object is associated with anathor cultural object."@en ;
                   
                   rdfs:range chc:CulturalObject ;
                   
                   rdfs:domain chc:CulturalObject .





