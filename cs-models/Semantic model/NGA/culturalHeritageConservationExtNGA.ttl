             sec:allowInheritLibraryPermissions "true"^^xsd:boolean ;
             
             dc:creator "Lidiya Dimitrova" ;
             
             skos:definition "Ephemera are any transitory written or printed matter meant for eventual repression (or not meant to be retained or preserved). However in the museum or archives context, ephemera must be preserved and accounted for." .



###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/NGA#Frame

nga:Frame rdf:type owl:Class ;
          
          rdfs:label "Frame"@en ;
          
          rdfs:subClassOf chc:CulturalObject ;
          
          sec:allowInheritParentPermissions "false"^^xsd:boolean ;
          
          emf:isSearchable "true"^^xsd:boolean ;
          
          sec:allowInheritLibraryPermissions "true"^^xsd:boolean ;
          
