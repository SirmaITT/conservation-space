###  http://www.sirma.com/ontologies/2016/02/culturalHeritageConservation/IMA#duplicates

ima:duplicates rdf:type owl:ObjectProperty ,
                        owl:SymmetricProperty ;
               
               rdfs:label "Duplicates"@en ;
               
               emf:isSearchable "true"^^xsd:boolean ;
               
               dc:creator "Lidiya Dimitrova" ;
               
               skos:definition "Indicates that the cultural object duplicates another cultural object."@en ;
               
               rdfs:domain chc:CulturalObject ;
               
               rdfs:range chc:CulturalObject .





