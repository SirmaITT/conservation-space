<?xml version="1.0"?>
<definition id="ngaActivity" parentId="GEO10001" isAbstract="true" type="object">
    <fields>
		<!-- headers -->
		<field name="default_header" type="ANY" label="ACTIVITY_DEFAULT_HEADER_NGA" />
        <field name="compact_header" type="an..4096"  label="ACTIVITY_COMPACT_HEADER_NGA" />
        <field name="breadcrumb_header" type="an..4096" label="ACTIVITY_BREADCRUMB_HEADER_NGA" />
		<field name="tooltip_header" type="an..4096" label="ACTIVITY_TOOLTIP_HEADER_NGA" />
		
		<field name="identifier" type="an..50" displayType="system" />
        <field name="type" type="an..50" displayType="system" />
        <field name="title" type="an..180" displayType="system"/>
		<field name="description" type="an..1024" displayType="system" />
		<field name="createdOn" 	type="dateTime" displayType="system" />
    	<field name="createdBy" 	type="uri" displayType="system" />
        <field name="modifiedOn" 	type="dateTime" displayType="system" />
        <field name="modifiedBy" 	type="uri" displayType="system" />
		<field name="status" 		type="an..50"  displayType="system" />
		<field name="emf:version" 		type="an..10" displayType="system" />
		<field name="parentOf" type="uri" displayType="system" />
		<field name="hasParent" type="uri" displayType="system" />
		<field name="hasThumbnail" type="uri" displayType="system" />
		<field name="outcome" type="an..50" displayType="system" />
		<field name="completedOn" type="datetime" displayType="system" />
		
		<field name="integrated" type="boolean" displayType="system" />
		<field name="uri" type="an..50" displayType="system" />
		<field name="hasRevision" type="uri" displayType="system" />
		<field name="isRevisionOf" type="uri" displayType="system" />
		<field name="lastPublishedRevision" type="uri" displayType="system" />
		<field name="lastRevision" type="uri" displayType="system" />
		<field name="nextRevision" type="uri"  displayType="system" />
		<field name="previousRevision" type="uri" displayType="system" />
		<field name="partOf" type="uri" displayType="system" />
		<field name="hasChild" type="uri" displayType="system" />
    </fields>
	
	<regions>	
		<region id="generalActivityInformation" order="10" displayType="editable" label="ngaActivity.generalActivityInformation.region.label">
			<fields>
			    <field name="type" type="an..180" order="110" codelist="CL3" label="GEO10001.type" tooltip="nga.type.tooltip" mandatory="false" displayType="readonly" dmsType="emf:type" >
					<value>NGACA</value>
				</field>
				<field name="title" type="an..180" label="nga.title.label" tooltip="nga.title.tooltip" mandatory="true" displayType="editable" previewEmpty="false" order="118" dmsType="cm:title" uri="dcterms:title" />				
				<field name="description" type="an..5000" order="120" label="GEO10001.description" tooltip="nga.description.tooltip" displayType="editable" dmsType="cm:description" uri="dcterms:description" />
			</fields>
		</region>
		<region id="timeAndEffort" order="20" displayType="editable" label="ngaActivity.timeAndEffort.region.label">
			<fields>
				<field name="plannedStartDate" type="date" order="200" label="nga.plannedStartDate.label" tooltip="nga.plannedStartDate.tooltip" mandatory="false" displayType="editable" previewEmpty="false" uri="emf:plannedStartDate">
					<value>${today}</value>
				</field>
				<field name="actualStartDate" type="date" order="205" label="ngaActivity.actualStartDate.label" tooltip="ngaActivity.actualStartDate.tooltip" displayType="editable" uri="emf:actualStartDate" >
					<value>${today}</value>
				</field>	
				<field name="plannedEndDate" type="date" order="210" label="nga.plannedEndDate.label" tooltip="nga.plannedEndDate.tooltip" mandatory="false" displayType="editable" previewEmpty="false" uri="emf:plannedEndDate"/>
				<field name="actualEndDate" type="date" order="215" label="ngaActivity.actualEndDate.label" tooltip="ngaActivity.actualEndDate.tooltip" displayType="editable" uri="emf:actualEndDate" />
				<field name="estimatedEffortHours" type="n..10" order="220" label="ngaActivity.estimatedEffortHours.label" tooltip="ngaActivity.estimatedEffortHours.tooltip" displayType="editable" previewEmpty="true" uri="emf:estimatedEffort" />
			</fields>
		</region>
		<region id="systemInformation" order="30" label="nga.systemInformation.region.label">
			<fields>
				<field name="status" type="an..50" codelist="CL106" label="nga.status.label" tooltip="nga.status.tooltip" mandatory="false" displayType="readOnly" order="302" previewEmpty="false" dmsType="emf:status" uri="emf:status" />
				<field name="emf:version" type="an..10" label="object.version" tooltip="nga.version.tooltip" displayType="readOnly" order="305" dmsType="FORBIDDEN" uri="emf:version"/>
				<field name="createdOn" type="dateTime" label="nga.createdOn.label" tooltip="nga.createdOn.tooltip" displayType="readOnly" order="310" dmsType="-cm:created" uri="emf:createdOn" />
				<field name="createdBy" type="uri" label="nga.createdBy.label" tooltip="nga.createdBy.tooltip" displayType="readonly" order="315" dmsType="-cm:creator" uri="emf:createdBy" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:User</control-param>
					</control>
				</field>
				<field name="modifiedOn" type="datetime" label="nga.modifiedOn.label" tooltip="nga.modifiedOn.tooltip" displayType="readOnly" order="320" previewEmpty="false" dmsType="-cm:modified" uri="emf:modifiedOn" />
				<field name="modifiedBy" type="uri" label="nga.modifiedBy.label" tooltip="nga.modifiedBy.tooltip" displayType="readOnly" order="325" previewEmpty="false" dmsType="-cm:modifier" uri="emf:modifiedBy">
					<control id="PICKER">
						<control-param id="range" name="range">emf:User</control-param>
					</control>
				</field>
				<field name="lockedBy" type="uri" label="nga.lockedBy.label" tooltip="nga.lockedBy.tooltip" displayType="readonly" order="330" dmsType="FORBIDDEN" previewEmpty="false">
					<control id="PICKER">
						<control-param id="range" name="range">emf:User</control-param>
					</control>
				</field>
				<field name="partOf" type="uri" label="nga.partOf.label" tooltip="nga.partOf.tooltip" mandatory="false" displayType="readOnly" order="818" multiValued="true" previewEmpty="false" uri="ptop:partOf" >
					<control id="PICKER">
						<control-param id="range" name="range"></control-param>
					</control>
				</field>
				<field name="hasChild" type="uri" label="nga.hasChild.label" tooltip="nga.hasChild.tooltip" mandatory="false" displayType="readOnly" order="820" multiValued="true" previewEmpty="false" uri="emf:hasChild" >
					<control id="PICKER">
						<control-param id="range" name="range"></control-param>
					</control>
				</field>
			</fields>
		</region>
		
		<region id="ngaActivityDefinitionRelationships" order="40" displayType="editable" label="nga.relationships.region.label">
			<fields>
				<field name="attachmentOf" type="uri" label="nga.attachmentOf.label" tooltip="nga.attachmentOf.tooltip" mandatory="false" displayType="editable" multiValued="true" order="400" previewEmpty="false" uri="chc:attachmentOf" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:Document,emf:Audio,emf:Video,chc:CulturalObject,chc:Sample,emf:Image,emf:Task,emf:Project,chc:ConservationActivity,emf:BusinessProcess</control-param>
					</control>
				</field>
				<field name="hasAttachment" type="uri" label="nga.hasAttachment.label" tooltip="nga.hasAttachment.tooltip" mandatory="false" displayType="editable" multiValued="true" order="402" previewEmpty="false" uri="chc:hasAttachment" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:Document,emf:Audio,emf:Video,chc:CulturalObject,chc:Sample,emf:Image,emf:Task,emf:Project,chc:ConservationActivity,emf:BusinessProcess</control-param>
					</control>
				</field>
				<field name="emf:hasWatcher" type="uri" label="nga.hasWatcher.label" tooltip="nga.hasWatcher.tooltip" mandatory="false" displayType="editable" order="404" multiValued="true" previewEmpty="false" uri="emf:hasWatcher" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:User</control-param>
					</control>
				</field>
				<field name="hasThumbnail" type="uri" label="nga.hasThumbnail.label" tooltip="nga.hasThumbnail.tooltip" mandatory="false" displayType="readOnly" order="406" previewEmpty="false" uri="emf:hasThumbnail" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:Image</control-param>
					</control>
				</field>
				<field name="parentOf" type="uri" label="nga.parentOf.label" tooltip="nga.parentOf.tooltip" mandatory="false" displayType="readOnly" multiValued="true" order="408" previewEmpty="false" uri="emf:parentOf" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:Document,emf:Audio,emf:Video,chc:CulturalObject,chc:Sample,emf:Image,emf:Task,emf:Project,chc:ConservationActivity,emf:BusinessProcess</control-param>
					</control>
				</field>
				<field name="hasParent" type="uri" label="nga.hasParent.label" tooltip="nga.hasParent.tooltip" mandatory="false" displayType="readOnly" order="410" previewEmpty="false" uri="emf:hasParent" >
					<control id="PICKER">
						<control-param id="range" name="range">emf:Document,emf:Audio,emf:Video,chc:CulturalObject,chc:Sample,emf:Image,emf:Task,emf:Project,chc:ConservationActivity,emf:User,emf:BusinessProcess</control-param>
					</control>
				</field>
			</fields>
		</region>
	</regions>
	
	<transitions>
		<transition id="editDetails" order="10" />
		<transition id="addCulturalObject" order="11" label="nga.addCulturalObject.action.label" tooltip="nga.addCulturalObject.action.tooltip" eventId="addCulturalObject" purpose="addRelation">
			<fields>
				<field name="processesByToConfig" type="an10">
					<value>
						{
							"predefinedTypes" : ["chc:CulturalObject"],
							"selection" : "multiple",
							"relation" : ["emf:processes"],
							"implicitParams" : {
							"removeExisting" : false
							}
						}
					</value>
					<control id="configuration" />
				</field>
			</fields>
		</transition>
		<transition id="reopen" order="12" label="nga.reopen.action.label" tooltip="nga.reopen.action.tooltip" eventId="restart" purpose="transition" immediate="true" confirmation="reopen.confirm" >
			<fields>
				<field name="sendMail" type="an..200" displayType="system" mandatory="false">
					<value>
						<![CDATA[	
							var subType = codelist.getDescription(239, root.get('subType'), 'en');
							var title = root.get("title");
							var subject = '(Reopened) Conservation Activity: ' + subType + ' - ' + title;
							
							//extract warchers and createdBy users
							var watchers = root.get('emf:hasWatcher');
							var createdBy = root.get('createdBy');
							var recipients = [];
							var i = 0;
							if (watchers != null){
								for(i = 0; i < watchers.length; i++) {
									recipients[i] = watchers[i].toString();
								}
							}	
							recipients.push(createdBy.toString());
							mail.sendNotifications(root, security.getCurrentLoggedUser(), subject, "email_reopen_object", recipients, context);
						]]>
					</value>
					<control id="SCRIPT">
						<control-param id="phase" name="phase">after</control-param>
						<control-param id="async " name="async ">true</control-param>
						<control-param id="persistent" name="persistent">false</control-param>
					</control>
				</field>
			</fields>
		</transition>
		<transition id="complete" order="14" label="nga.complete.action.label" tooltip="complete.tooltip" eventId="complete" purpose="transition" immediate="true" confirmation="complete.confirm" >
			<fields>
				<field name="sendMail" type="an..200" displayType="system" mandatory="false">
					<value>
						<![CDATA[	
							var subType = codelist.getDescription(239, root.get('subType'), 'en');
							var title = root.get('title');
							var subject = '(Complete) Conservation Activity: ' + subType + ' - ' + title;
							
							//extract warchers and crearedBy users
							var watchers = root.get('emf:hasWatcher');
							var createdBy = root.get('createdBy');
							var recipients = [];
							var i = 0;
							if (watchers != null){
								for(i = 0; i < watchers.length; i++) {
									recipients[i] = watchers[i].toString();
								}
							}
							recipients.push(createdBy.toString());
							mail.sendNotifications(root, security.getCurrentLoggedUser(), subject, "email_complate_object", recipients, context);
						]]>
					</value>
					<control id="SCRIPT">
						<control-param id="phase" name="phase">after</control-param>
						<control-param id="async " name="async ">true</control-param>
						<control-param id="persistent" name="persistent">false</control-param>
					</control>
				</field>
			</fields>
		</transition>
		<transition id="reassign" order="16" label="nga.reassign.action.label" tooltip="reassign.tooltip" eventId="reassign" purpose="addRelation">
			<fields>
	 			<field name="attachToConfig" type="an10">
	 				<value>
	 					{
							"predefinedTypes" : ["emf:User"],
							"selection" : "single",
							"relation" : ["emf:hasAssignee"],
							"implicitParams" : {
								"removeExisting" : true
			  				}
						}
	 				</value>
	 				<control id="configuration" />
				</field>
				<field name="beforeReasing" type="an..200" displayType="system" mandatory="false">
					<value>
						<![CDATA[
							//add to context old assignee
							context.put('previousAsignee', root.get('assignee'));
						]]>
					</value>
					<control id="SCRIPT">
						<control-param id="phase" name="phase">before</control-param>
						<control-param id="async" name="async">false</control-param>
						<control-param id="persistent" name="persistent">false</control-param>
					</control>
				</field>
				<field name="afterReasing" type="an..200" displayType="system" mandatory="false">
					<value>
						<![CDATA[
							var subType = codelist.getDescription(239, root.get('subType'), 'en');
							var title = root.get("title");
							var subject = '(Reassigned) Conservation Activity: ' + subType + ' - ' + title;
							
							//extract watchers, assignee, createdBy and old assignee
							var recipients = [];
							var watchers = root.get('emf:hasWatcher');
							if(watchers != null){
								for(var i = 0; i < watchers.length; i++){
								recipients.push(watchers[i].toString());
								}
							}
							recipients.push(root.get('assignee').toString());
							recipients.push(root.get('createdBy').toString());
							recipients.push(context.get('previousAsignee'));
							mail.sendNotifications(root, security.getCurrentLoggedUser(), subject, "email_reassigned_object", recipients, context, null);
						]]>
					</value>
					<control id="SCRIPT">
						<control-param id="phase" name="phase">after</control-param>
						<control-param id="async" name="async">false</control-param>
						<control-param id="persistent" name="persistent">false</control-param>
					</control>
				</field>
			</fields>
		</transition>
		<transition id="addAttachment" order="18" label="nga.addAttachment.action.label" tooltip="nga.addAttachment.action.tooltip" eventId="addAttachment" purpose="addRelation" >
			<fields>
				<field name="attachToConfig" type="an10">
					<value>
						{
							"predefinedTypes" : ["emf:Project","chc:ConservationActivity","emf:Document","emf:Media","emf:Task","chc:CulturalObject","emf:Image","emf:Audio","emf:Video","chc:Sample","emf:BusinessProcess"],
							"selection" : "multiple",
							"relation" : ["chc:hasAttachment"],
							"implicitParams" : {
								"removeExisting" : false
							}
						}
					</value>
					<control id="configuration" />
				</field>
			</fields>
		</transition>	
		<transition id="createDocument" order="19" label="nga.createDocument.action.label" tooltip="nga.createDocument.action.tooltip" eventId="createDocument" purpose="createInstance">
      		<fields>
				<field name="createDocConfig" type="an10">
					<value>
   						{ 
							"predefinedTypes" : ["emf:Document"],  
							"contextSelectorDisabled": true
    					}
  			 	  </value>
 		      	  <control id="configuration" />
 		   		</field>
 		   </fields>
		</transition>
		<transition id="createTask" order="20" label="nga.createTask.action.label" tooltip="nga.createSubTask.action.tooltip" eventId="createTask" purpose="createInstance">
      		<fields>
				<field name="createTaskConfig" type="an10">
					<value>
						{ 
							"predefinedTypes" : ["emf:Task"],
							"contextSelectorDisabled": true
						}
  			 	  </value>
 		      	  <control id="configuration" />
 		   		</field>
 		   </fields>
		</transition>
		<!-- not used, only lable is change as the actions are visible in manage actions per roles -->
		<transition id="print" order="18" label="nga.print.label"/>
		<transition id="exportPDF" order="20" label="nga.exportPDF.label"/> 
		<transition id="exportWord" order="22" label="nga.exportWord.label"/>
		<!-- ******************************************************************** -->
		<transition id="printTab" order="21" label="print.label" />
		<transition id="exportTabPDF" order="22" label="exportPDF.label" />
		<transition id="exportTabWord" order="25" label="exportWord.label" />
		<transition id="clone" order="26" />
		<transition id="move" order="28" >
			<fields>
				<field name="sendMail" type="an..200" displayType="system" mandatory="false">
					<value>
						<![CDATA[	
							var subType = codelist.getDescription(239, root.get('subType'), 'en');
							var title = root.get('title');
							var subject = '(Moved) Conservation Activity: ' + subType + ' - ' + title;
							
							//extract warchers and crearedBy users
							var watchers = root.get('emf:hasWatcher');
							var createdBy = root.get('createdBy');
							var recipients = [];
							var i = 0;
							if (watchers != null){
								for(i = 0; i < watchers.length; i++) {
									recipients[i] = watchers[i].toString();
								}
							}	
							recipients.push(createdBy.toString());
							mail.sendNotifications(root, security.getCurrentLoggedUser(), subject, "email_move_object", recipients, context);
						]]>
					</value>
					<control id="SCRIPT">
						<control-param id="phase" name="phase">after</control-param>
						<control-param id="async" name="async">true</control-param>
						<control-param id="persistent" name="persistent">false</control-param>
					</control>
				</field>
			</fields>
		</transition>	
		<transition id="lock" order="30" />
		<transition id="unlock" order="32" />
		<transition id="addWatchers" order="34" label="addWatchers.label" tooltip="addWatchers.tooltip" eventId="addWatchers" purpose="addRelation" >
			<fields>
				<field name="addAttachmentConfig" type="an10">
					<value>						
						{
							"predefinedTypes" : ["emf:User"],
							"selection" : "multiple",
							"relation" : ["emf:hasWatcher"],
							"implicitParams" : {
								"removeExisting" : false
							}
						}
					</value>
					<control id="configuration" />
				</field>
			</fields>
		</transition>
		<transition id="addThumbnail" order="36" />
		<transition id="saveAsTemplate" order="38" />	
		<transition id="managePermissions" order="40" />			
		<transition id="delete" order="42" confirmation="deleteCS.confirm" >
			<fields>
				<field name="sendMail" type="an..200" displayType="system" mandatory="false">
                    <value>
						<![CDATA[	
							var subType = codelist.getDescription(239, root.get('subType'), 'en');
							var title = root.get('title');
							var subject = '(Deleted) Conservation Activity: ' + subType + ' - ' + title;
							
							//extract warchers and crearedBy users
							var watchers = root.get('emf:hasWatcher');
							var createdBy = root.get('createdBy');
							var recipients = [];
							var i = 0;
							if (watchers != null){
								for(i = 0; i < watchers.length; i++) {
									recipients[i] = watchers[i].toString();
								}
							}
							recipients.push(createdBy.toString());
							mail.sendNotifications(root, security.getCurrentLoggedUser(), subject, "email_delete_object", recipients, context);
						]]>
                    </value>
                    <control id="SCRIPT">
                        <control-param id="phase" name="phase">after</control-param>
                        <control-param id="async" name="async">true</control-param>
                        <control-param id="persistent" name="persistent">false</control-param>
                    </control>
                </field>
			</fields>
		</transition>
	</transitions>
	<stateTransitions>
		<stateTransition from="INIT" transition="create" to="IN_PROGRESS"/>
		<stateTransition from="INIT" transition="clone" to="IN_PROGRESS"/>
		
		<stateTransition from="IN_PROGRESS" transition="saveAsTemplate" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="editDetails" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="move" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="managePermissions" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="addThumbnail" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="lock" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="unlock" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="printTab" to="IN_PROGRESS" />
        <stateTransition from="IN_PROGRESS" transition="exportTabPDF" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="exportTabWord" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="clone" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="reassign" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="addAttachment" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="addCulturalObject" to="IN_PROGRESS"/>
		<stateTransition from="IN_PROGRESS" transition="addWatchers" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="createTask" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="createDocument" to="IN_PROGRESS" />
		<stateTransition from="IN_PROGRESS" transition="createInContext" to="IN_PROGRESS"/>
		
		<stateTransition from="IN_PROGRESS" transition="delete" to="DELETED"/>
		
		<stateTransition from="IN_PROGRESS" transition="complete" to="COMPLETED"/>
		<stateTransition from="COMPLETED" transition="managePermissions" to="COMPLETED"/>
		<stateTransition from="COMPLETED" transition="printTab" to="COMPLETED" />
        <stateTransition from="COMPLETED" transition="exportTabPDF" to="COMPLETED" /> 
		<stateTransition from="COMPLETED" transition="exportTabWord" to="COMPLETED" />
		<stateTransition from="COMPLETED" transition="clone" to="COMPLETED" />
		
		<stateTransition from="COMPLETED" transition="reopen" to="IN_PROGRESS" />
		
		<stateTransition from="*" transition="delete" to="DELETED" />
	</stateTransitions>
	
	<labels>
		<label id="ACTIVITY_TOOLTIP_HEADER_NGA">
    		<value lang="en"><![CDATA[${eval(#{eval(
				<span class="thumbnail-content">
					<img class="thumbnail" src="${thumbnailUri(64)}" />
				</span>
				<span class="tooltip-data">
					<span class="tooltip-header">
						<a href="${link(currentInstance)}" class="instance-link" data-status="${get([status])}" data-id="${id.db}" data-uid="${id.uid}">
							<span data-property="title">${get([title])}</span>
						</a>
					</span>
					<span class="tooltip-row">
						<label>Status: </label>
						<span data-property="status">${CL([status])}</span>
					</span>
					<span class="tooltip-row">
                        <label>Department: </label>
                        <span data-property="department">${CL([department])}</span>
                    </span>
					${if(${get([plannedStartDate])}<>null).then(
					<span class="tooltip-row">
                        <label>Planned start date: </label>
                        <span data-property="plannedStartDate">${date([plannedStartDate]).format(MM.dd.yyyy)}</span>
                    </span>
					)}
					${if(${get([plannedEndDate])}<>null).then(
					<span class="tooltip-row">
                        <label>Planned end date: </label>
                        <span data-property="plannedEndDate">${date([plannedEndDate]).format(MM.dd.yyyy)}</span>
                    </span>
					)}
					${if(${get([estimatedEffortHours])}<>null).then(
					<span class="tooltip-row">
                        <label>Estimated effort hours: </label>
                        <span data-property="estimatedEffortHours">${get([estimatedEffortHours])}</span>
                    </span>
					)}
					<span class="tooltip-row">
						<label>System modified by: </label>
						<span data-property="modifiedBy">${user(${get([modifiedBy])})}</span>
					</span>
					<span class="tooltip-row">
						<label>System modified on: </label>
						<span data-property="modifiedOn">${date([modifiedOn]).format(MM.dd.yyyy, HH:mm)}</span>
					</span>
				</span>
			)})}]]></value>
    	</label>
		<label id="ACTIVITY_DEFAULT_HEADER_NGA">
			<value lang="en"><![CDATA[${eval(
					${var.href= ${if(${get([emf:isDeleted])} == false).then(href="${link(currentInstance)}")} }
					<span><img src="${thumbnailUri(64)}"/></span>
					<span class="truncate-element">
						<span data-activityType="${get([activityType])}">${CL([activityType])}</span>&nbsp;
						(<span data-property="status">${CL([status])}</span>)
					</span>
					<br />
					<span>
						<a class="${get([status])} instance-link ${if(${get([emf:isDeleted])} == true).then(instance-deleted).else(has-tooltip)}" ${var.href} uid="${id.uid}">
							${if(${get([emf:isDeleted])} == true).then(<span>(Deleted)</span>&nbsp;)}
							<span data-property="title">${get([title])}</span>
						</a>&nbsp;
						<span data-property="department">${CL([department])}</span>
					<br />
					<span class="banner label label-warning">${if(${get([lockedBy])} <> null).then(${label(object.lockedBy)}${user([lockedBy])})}</span>
					</span>
			)}]]></value>
		</label>
        <label id="ACTIVITY_COMPACT_HEADER_NGA">	
		   <value lang="en"><![CDATA[${eval(
			${var.href= ${if(${get([emf:isDeleted])} == false).then(href="${link(currentInstance)}")} }
			<span><img src="${thumbnailUri(16)}"/></span>
			<span class="truncate-element">
				${if(${get([lockedBy])} <> null).then(<span class="lock-label"><i class="fa fa-lock fa-lg"></i></span>)}
				<a class="${get([status])} instance-link ${if(${get([emf:isDeleted])} == true).then(instance-deleted).else(has-tooltip)}" ${var.href} uid="${id.uid}" style="vertical-align:baseline;">
					${if(${get([emf:isDeleted])} == true).then(<span>(Deleted)</span>&nbsp;)}
					<span data-property="activityType">${CL([activityType])}</span>:&nbsp;
					<span data-property="title">${get([title])}</span>
				</a>&nbsp;
				(<span data-property="status">${CL([status])}</span>)
			</span>
		)}]]></value>
		</label>
		
		<label id="ACTIVITY_BREADCRUMB_HEADER_NGA">
            <value lang="en"><![CDATA[${eval(
            ${var.href= ${if(${get([emf:isDeleted])} == false).then(href="${link(currentInstance)}")} }
			<span><img src="${thumbnailUri(16)}"/></span>
			<span>
				${if(${get([lockedBy])} <> null).then(<span class="lock-label"><i class="fa fa-lock fa-lg"></i></span>)}
				<span data-property="activityType">${CL([activityType])}</span>&nbsp;
				<a class="${get([status])} instance-link ${if(${get([emf:isDeleted])} == true).then(instance-deleted).else(has-tooltip)}" ${var.href} uid="${id.uid}">
					${if(${get([emf:isDeleted])} == true).then(<span>(Deleted)</span>)}
					<span data-property="activityType">${CL([activityType])}</span>&nbsp;
					<span data-property="title">${get([title])}</span>
			</span>		
            </a>)}]]></value>
		</label>
	</labels>
</definition>
