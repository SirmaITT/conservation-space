<roles>
	<role name="CONSUMER" priority="5">
		<include>consumer_document</include>
		<include>consumer_object</include>
		<include>consumer_user</include>
	</role>
	<role name="consumer_document" external="false">
		<permission name="edit">
			<actions applyTo="document">
				<action name="attachTo"></action>
			</actions>
		</permission>
	</role>
	<role name="consumer_object" external="false">
		<permission name="edit">
			<actions applyTo="object">
				<action name="attachTo"></action>
				<action name="download"></action>
				<action name="saveAsTemplate"></action>
			</actions>
		</permission>
	</role>
	<role name="consumer_user" external="false">
		<permission name="edit">
			<actions applyTo="user">
				<action name="editDetails"></action>
				<action name="addThumbnail"></action>	
			</actions>
		</permission>
	</role>
</roles>